.swiper-progress
  position relative !important
  height: 6px !important
  overflow: hidden
  background: #747474
  margin: 18px 0 0 0
  border-radius: 10px !important
  +above($breakpoints.sm)
    display: none
  .swiper-pagination-progressbar-fill
    border-radius: 10px !important
    background: #F3B88E
